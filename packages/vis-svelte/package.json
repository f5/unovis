{
  "name": "@unovis/svelte",
  "version": "1.0.0-beta.1",
  "description": "Svelte wrapper for Unovis",
  "license": "Apache-2.0",
  "main": "./index.js",
  "types": "./index.d.ts",
  "svelte": "./index.js",
  "scripts": {
    "dev": "rm -rf build; rollup -c --watch",
    "build": "npm run check && rm -rf dist; rollup -c",
    "check": "svelte-check --ignore \"src-demo\" --tsconfig ./tsconfig.json",
    "generate": "rollup -c ./autogen/rollup.config.js; node .autogen.js; rm .autogen.js",
    "publish-dist": "cp ./package.json ./dist; cp ../../LICENSE ./dist; cd ./dist; npm publish; rm -rf .cache"
  },
  "peerDependencies": {
    "@unovis/ts": "1.0.0-beta.1",
    "svelte": "^3.48.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@tsconfig/svelte": "^2.0.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "sveld": "^0.15.1",
    "svelte": "^3.48.0",
    "rollup": "^2.61.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dev": "^2.0.4",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "tslib": "^2.3.1",
    "ttypescript": "^1.5.13",
    "typescript": "~4.2.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
