{
  "name": "@unovis/react",
  "description": "Modular data visualization framework for React, Angular, Svelte, Vue, Solid, and vanilla TypeScript or JavaScript",
  "version": "1.5.1",
  "packageManager": "npm@10.9.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/f5/unovis.git",
    "directory": "packages/react"
  },
  "homepage": "https://unovis.dev",
  "bugs": {
    "url": "https://github.com/f5/unovis/issues"
  },
  "keywords": [
    "react",
    "data visualization",
    "maps",
    "charts",
    "graphs"
  ],
  "author": "Nikita Rokotyan, F5 Inc. <nikita@f5.com> (https://rokotyan.com)",
  "maintainers": [
    "Nikita Rokotyan <nikita@f5.com> (https://rokotyan.com)",
    "Rebecca Bol <r.bol@f5.com>"
  ],
  "license": "Apache-2.0",
  "main": "./index.js",
  "types": "./index.d.ts",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "vite build --mode production",
    "build:types": "tsc --project tsconfig.lib.json --emitDeclarationOnly",
    "watch": "concurrently \"vite build --mode development --watch\" \"npm run build:types --watch\"",
    "tsc": "tsc",
    "generate": "rollup -c ./autogen/rollup.config.js; node .autogen.cjs; rm .autogen.cjs"
  },
  "peerDependencies": {
    "@unovis/ts": "1.5.1",
    "react": ">=16.8.0 || ^17 || ^18 || ^19",
    "react-dom": ">=16.8.0 || ^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/node": "^16.11.17",
    "@types/react": ">=16.8.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "concurrently": "^9.1.2",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "tslib": "^2.3.1",
    "typescript": "~4.2.4",
    "vite": "^6.2.2",
    "vite-plugin-dts": "~3.5.3",
    "vite-plugin-externalize-deps": "^0.9.0"
  }
}
