{
  "name": "@volterra/vis-react",
  "version": "0.9.21",
  "description": "React wrapper for @volterra/vis",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.lib.json --emitDeclarationOnly && rollup --config",
    "tsc": "tsc",
    "generate": "rollup -c ./autogen/rollup.config.js; node .autogen.js; rm .autogen.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish-dist": "cp ./{.npmrc,package.json} ./dist; cp ../../LICENSE ./dist; cd ./dist; npm publish; rm -rf .cache"
  },
  "author": "Nikita Rokotyan",
  "license": "MIT",
  "publishConfig": {
    "cache": ".cache/.npm",
    "@volterra:registry": "https://gitlab.com/api/v4/projects/31255670/packages/npm/"
  },
  "peerDependencies": {
    "@emotion/css": "^11.7.1",
    "@volterra/vis": "^0.9.21",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@emotion/css": "^11.7.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-docs": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/builder-webpack5": "6.4.9",
    "@storybook/manager-webpack5": "6.4.9",
    "@storybook/react": "6.4.9",
    "@types/react": "^17.0.22",
    "@types/node": "^16.11.17",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.31.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tslib": "^2.3.1",
    "ttypescript": "^1.5.13",
    "typescript": "~4.2.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
