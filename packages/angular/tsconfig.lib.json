/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/lib",
    "target": "ES2022",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": ["topojson-specification"],
    "lib": [
      "dom",
      "ES2022"
    ],
    "plugins": [
    ],
    "allowSyntheticDefaultImports": true,
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ]
}
